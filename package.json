{
  "name": "locky",
  "version": "0.0.1",
  "description": "More options to lock/unlock the desktop",
  "license": "GPL-3.0-or-later",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/ssshojaei/locky"
  },
  "homepage": "https://github.com/ssshojaei/locky#readme",
  "bugs": {
    "url": "https://github.com/ssshojaei/locky/issues"
  },
  "author": "SSShojaei <ordinarysaleh@pm.me> (https://ssshojaei.ir)",
  "keywords": [
    "lock",
    "unlock",
    "fingerprint"
  ],
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "browserslist": [
    "last 4 versions"
  ],
  "main": "main.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.build.config.js --mode production",
    "package": "npm run build",
    "postpackage": "electron-packager ./ --out=./builds"
  },
  "dependencies": {
    "electron-json-config": "^1.5.3",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "rsuite": "^4.8.0",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "babel-minify-webpack-plugin": "^0.3.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "electron": "^13.6.6",
    "electron-devtools-installer": "^3.0.0",
    "electron-packager": "^15.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^5.1.1",
    "style-loader": "^1.2.0",
    "svg-sprite-loader": "^5.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
